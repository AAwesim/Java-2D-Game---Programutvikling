<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Core.menyController">
   <children>
      <Button fx:id="startButton" layoutX="270.0" layoutY="110.0" onAction="#setGameScene" prefWidth="100" text="Start" />
      <Button fx:id="hjelpButton" layoutX="270.0" layoutY="150.0" onAction="#setHjelpScene"  prefWidth="100" text="Hjelp" />
      <Button fx:id="exitButton" layoutX="270.0" layoutY="190.0" onAction="#exit" prefWidth="100" text="Avslutt" />
   </children>
</AnchorPane>
